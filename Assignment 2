# Base class
class Vehicle:
    def move(self):
        """This method will be overridden in each subclass"""
        pass


# Subclass: Car
class Car(Vehicle):
    def move(self):
        print("Driving üöó")


# Subclass: Plane
class Plane(Vehicle):
    def move(self):
        print("Flying ‚úàÔ∏è")


# Subclass: Boat
class Boat(Vehicle):
    def move(self):
        print("Sailing ‚õ¥Ô∏è")


# Example usage
vehicles = [Car(), Plane(), Boat()]

for vehicle in vehicles:
    vehicle.move()  # Each vehicle responds differently
